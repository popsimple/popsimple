<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.3.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.3.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='Login'>
	<inherits name="com.google.gwt.user.User" />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

    <inherits name='com.google.common.base.Base'/>
    <inherits name="com.google.common.collect.Collect" />

	<inherits name='com.project.website.shared.Shared' />
	<inherits name='com.project.shared.Shared' />

	<set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true"/>
	<!-- for debugging only -->
	<set-configuration-property name="CssResource.style"
		value="pretty" />

	<source path="client" />

	<entry-point class="com.project.website.authentication.client.Login"></entry-point>
</module>