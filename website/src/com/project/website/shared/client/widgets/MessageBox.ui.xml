<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res' type='com.project.website.shared.client.widgets.authentication.resources.AuthenticationResources'/>
	<g:HTMLPanel addStyleNames="{res.main.rootPanel}">
		<div>
			<g:Label ui:field="labelTitle" addStyleNames="{res.main.captionLabel}"></g:Label>
		</div>
		<g:HTMLPanel ui:field="containerPanel" addStyleNames="{res.main.containerPanel}">
			<g:FormPanel ui:field="messageBoxForm">
				<g:HTMLPanel addStyleNames="{res.main.registrationPanel}">
					<p>
						<g:Label ui:field="labelContent" addStyleNames="{res.main.formLabel}"></g:Label>
					</p>
					<p class="{res.main.buttonRow}">
						<g:SubmitButton ui:field="buttonYes" addStyleNames="{res.main.button}">Yes</g:SubmitButton>
                        <g:Button ui:field="buttonNo" addStyleNames="{res.main.button} ">No</g:Button>
					</p>
				</g:HTMLPanel>
			</g:FormPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>