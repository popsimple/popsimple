<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	 <ui:with field='res' type='com.project.website.canvas.client.resources.CanvasResources'/>
	<ui:style>

	</ui:style>
	<g:HTMLPanel ui:field="mainPanel">

       <g:DisclosurePanel addStyleNames="{res.main.mapOptionsMapTypePanel}" animationEnabled="true" open="true" >
		   <g:header addStyleNames="{res.main.mapOptionsSectionHeader}">Map type:</g:header>
		   <g:FlowPanel ui:field="mapTypesPanel"/>
       </g:DisclosurePanel>

	   <g:DisclosurePanel addStyleNames="{res.main.mapOptionsProvidersPanel}" animationEnabled="true" open="false" >
		   <g:header addStyleNames="{res.main.mapOptionsSectionHeader}">Map provider:</g:header>
		   <g:FlowPanel ui:field="providersPanel"/>
	   </g:DisclosurePanel>

	   <g:Button ui:field="doneButton">Done</g:Button>
	</g:HTMLPanel>
</ui:UiBinder>