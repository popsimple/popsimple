<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:with field='res' type='com.project.website.canvas.client.resources.CanvasResources' />
    <ui:style>

    </ui:style>
    <g:HTMLPanel ui:field="rootPanel" addStyleNames="{res.main.videoBox}">
        <g:Frame ui:field="videoFrame" addStyleNames="{res.main.videoFrame}"></g:Frame>
        <g:FlowPanel ui:field="optionsBar" addStyleNames="{res.main.videoOptionsBar} {res.main.toolFrameHoverDisplayBlockElement}">
            <g:Button ui:field="optionsLabel" addStyleNames="{res.main.link} {res.main.videoOptionsLabel} ">
                Choose a video...
            </g:Button>
        </g:FlowPanel>
    </g:HTMLPanel>
</ui:UiBinder>