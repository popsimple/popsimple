<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.header
			{
			}
		.frame
			{
				position: absolute;
				border-style: solid;
				border-width: 1px;
				display:inline-block;
			}
		.tool
			{
				position:relative;
				margin: 5px;
			}
	</ui:style>
	<g:HTMLPanel ui:field="framePanel" addStyleNames="{style.frame}">
		<g:HTMLPanel ui:field="toolPanel" addStyleNames="{style.tool}">
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 