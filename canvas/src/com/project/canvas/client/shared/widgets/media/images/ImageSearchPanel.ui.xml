	xmlns:my="urn:import:com.project.canvas.client.shared.widgets.media">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<my:MediaSearchPanel ui:field="mediaSearchPanel"/>
		</g:HTMLPanel>
		<div>
			<g:CheckBox ui:field="stretchXOption" name="imageStyle">Stretch width</g:CheckBox>
