<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:com.project.canvas.client"
	xmlns:myWorksheet="urn:import:com.project.canvas.client.worksheet"
	>
	
	<ui:with field='res' type='com.project.canvas.client.resources.CanvasResources'/>
	
	
	<g:HTMLPanel addStyleNames="{res.main.canvasContainer}" >
	   <my:Toolbox ui:field="toolbox"/>
	   <myWorksheet:WorksheetWidget ui:field="worksheetWidget"/>
	</g:HTMLPanel>
</ui:UiBinder> 